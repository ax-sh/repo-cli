import { run } from './<%= props.name %>.service'
import { exeCmdWithOutput, runFromPromiseWithErrorHandlerWrapper } from '../../lib';
vi.mock('../../lib/helpers/cmd/cli')

describe('[<%= props.name %>] service test', () => {
    it('should return results', async () => {
        const fn = vi.mocked(exeCmdWithOutput)
        fn.mockImplementation(async (args: string) => args)
        const mod = await import('./<%= props.name %>.service')
        expect(mod).toBeDefined()

        const result = await runFromPromiseWithErrorHandlerWrapper(mod.run<%= props.name %>())
        if (result.isErr()) {
            throw result.error
        }
        const out = result.value
        console.warn(out)
    });
});
